<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.wonder.mapper.TestMapper">
    <resultMap id="individualTicket" type="com.wonder.entity.individualTicketVo">
        <result column="ACCOUNT_ID" property="accountId" javaType="java.lang.String" jdbcType="VARCHAR" />
        <result column="USER_ACCOUNT_NAME" property="accountName" javaType="java.lang.String" jdbcType="VARCHAR" />
    </resultMap>

    <resultMap id="depositAccountLog" type="com.wonder.entity.DepositAccountLog">
        <result column="ID" property="logId" javaType="java.lang.String" jdbcType="VARCHAR" />
        <result column="COMPANY_NAME" property="accountName" javaType="java.lang.String" jdbcType="VARCHAR" />
        <result column="region" property="region" javaType="java.lang.String" jdbcType="VARCHAR" />
    </resultMap>

    <select id="individualTicketCount" resultType="java.lang.Integer">
        select count(1) from dmp_main_order_item i  join dmp_main_order o on o.id=i.order_id      join dmp_main_proxy_prod_set s on i.product_code=s.product_code and i.proxy_id=s.sales_proxy_id
    </select>

    <select id="individualTicketQuery" resultMap="individualTicket">
        select o.ACCOUNT_ID,o.USER_ACCOUNT_NAME from dmp_main_order_item i  join dmp_main_order o on o.id=i.order_id      join dmp_main_proxy_prod_set s on i.product_code=s.product_code and i.proxy_id=s.sales_proxy_id
    </select>

    <select id="depositAccountLogCount" resultType="java.lang.Integer">
        select count(1) from dmp_main_order_item i  join dmp_main_order o on o.id=i.order_id      join dmp_main_proxy_prod_set s on i.product_code=s.product_code and i.proxy_id=s.sales_proxy_id
    </select>

    <select id="depositAccountLogQuery" resultMap="depositAccountLog">
        select a.ID,a.COMPANY_NAME,case da.REGION when 'GZ' then '广州长隆' when 'ZH' then '珠海长隆' when 'QY' then '清远长隆' end region from DMP_MAIN_DEPOSIT_ACCOUNT_LOG log join DMP_MAIN_DEPOSIT_ACCOUNT da on da.ID = log.DEPOSIT_ACCOUNT_ID join DMP_MAIN_ACCOUNT a on a.ID = da.ACCOUNT_ID
    </select>



</mapper>